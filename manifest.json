{
    "manifest_version": 3,
    "name": "AI Studio Model Switcher",
    "version": "1.4.1",
    "description": "自动切换 AI Studio 的模型为指定模型",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://aistudio.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://aistudio.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}